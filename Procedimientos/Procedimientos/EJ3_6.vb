Module EJ3_6
    Sub Main()
        Dim N, S, C As Integer

        Console.WriteLine("POLICIA")
        N = Console.ReadLine()

        For N = 1 To N
            S = N + (N - C) + N

            C = C + 1
            Console.WriteLine("POLICIA " & S)
        Next
        Console.ReadKey()
    End Sub
End Module
